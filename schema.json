{
    "definitions": {},
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "http://example.com/root.json",
    "type": "array",
    "title": "The Root Schema",
    "items": {
        "$id": "#/items",
        "type": "object",
        "title": "The Items Schema",
        "required": [
            "Country",
            "depression",
            "Log_of_GDP_per_capita",
            "total_litres_of_pure_alcohol"
        ],
        "properties": {
            "Country": {
                "$id": "#/items/properties/Country",
                "type": "string",
                "title": "The Country Schema",
                "default": "",
                "examples": [
                    "Afghanistan"
                ],
                "pattern": "^(.*)$"
            },
            "depression": {
                "$id": "#/items/properties/depression",
                "type": "number",
                "title": "The Depression Schema",
                "default": 0.0,
                "examples": [
                    4.13634710028
                ]
            },
            "Log_of_GDP_per_capita": {
                "$id": "#/items/properties/Log_of_GDP_per_capita",
                "type": "number",
                "title": "The Log_of_gdp_per_capita Schema",
                "default": 0,
                "examples": [
                    134
                ]
            },
            "total_litres_of_pure_alcohol": {
                "$id": "#/items/properties/total_litres_of_pure_alcohol",
                "type": "number",
                "title": "The Total_litres_of_pure_alcohol Schema",
                "default": 0,
                "examples": [
                    0
                ]
            }
        }
    }
}