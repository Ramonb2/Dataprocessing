{
    "definitions": {},
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "http://example.com/root.json",
    "type": "array",
    "title": "Country Schema",
    "items": {
        "$id": "#/Coutries",
        "type": "object",
        "title": "Country Schema",
        "required": [
            "Country",
            "depression",
            "Log_of_GDP_per_capita",
            "total_litres_of_pure_alcohol"
        ],
        "properties": {
            "Country": {
                "$id": "Country",
                "type": "string",
                "title": "The Country Schema",
                "default": "",
                "examples": [
                    "Nederland"
                ],
                "pattern": "[^+@#%&amp;()&lt;&gt;?-]+",
                "maxLength": 47
            },
            "depression": {
                "$id": "depression",
                "type": "number",
                "title": "The Depression Schema",
                "default": 0.0,
                "examples": [
                    4.1
                ]
            },
            "Log_of_GDP_per_capita": {
                "$id": "Log_of_GDP_per_capita",
                "type": "number",
                "title": "The Log_of_gdp_per_capita Schema",
                "default": 0,
                "examples": [
                    134
                ],
                "maximum" : 152
            },
            "total_litres_of_pure_alcohol": {
                "$id": "total_litres_of_pure_alcohol",
                "type": "number",
                "title": "The Total_litres_of_pure_alcohol Schema",
                "default": 0,
                "examples": [
                    0
                ],
                "minimum" : 0
            }
        }
    }
}